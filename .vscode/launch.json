    "version": "0.2.0",
    "configurations": [
        {
            "name": "sample_app (release prod)",
            "cwd": "apps/sample_app",
            "request": "launch",
            "type": "dart",
            "program": "apps/sample_app/lib/main_production.dart",
            "flutterMode": "release",
            "args": [
                "--flavor", "production",
                "--target", "lib/main_production.dart"
            ]
        },
        {
            "name": "sample_app (debug staging)",
            "cwd": "apps/sample_app",
            "request": "launch",
            "type": "dart",
            "program": "apps/sample_app/lib/main_staging.dart",
            "flutterMode": "debug",
            "args": [
                "--flavor", "staging",
                "--target", "lib/main_staging.dart"
            ]
        }
    ]
}